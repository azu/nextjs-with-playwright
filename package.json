{
    "name": "nextjs-with-playwright",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "clean": "cross-env NODE_V8_COVERAGE=.v8-coverage node ./scripts/clean.js",
        "test:start": "cross-env NODE_V8_COVERAGE=.v8-coverage NODE_OPTIONS=--inspect=9229 next dev",
        "test:command": "npm run clean && npm run test:start",
        "test": "playwright test"
    },
    "license": "MIT",
    "dependencies": {
        "next": "^14.2.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@playwright/test": "^1.45.1",
        "@types/node": "^20.14.10",
        "@types/react": "^18.3.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.4",
        "eslint-config-plus": "^1.0.6",
        "monocart-reporter": "^2.6.0",
        "typescript": "^5.5.3"
    }
}